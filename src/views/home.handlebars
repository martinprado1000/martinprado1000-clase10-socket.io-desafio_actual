<h1>Vista Home, no trabaja con socket.io</h1>

  <table class="table table-sm table-dark table-striped" id="tableProducts">
  <thead>
    <tr>
      <th scope="col">#</th>
      <th scope="col">Titulo</th>
      <th scope="col">Descripcion</th>
      <th scope="col">Precio</th>
      <th scope="col">Imagen</th>
      <th scope="col">Code</th>
      <th scope="col">Categoria</th>
      <th scope="col">Cantidad</th>
    </tr>
  </thead>
  <tbody>
    {{#each data}}
    <tr id="celda">
      <th scope="row">{{this.id}}</th>
      <td>{{this.title}}</td>
      <td>{{this.description}}</td>
      <td>{{this.price}}</td>
      <td>{{this.thumbnail}}</td>
      <td>{{this.code}}</td>
      <td>{{this.category}}</td>
      <td>{{this.stock}}</td> 
      
    </tr>
    {{/each}}
  </tbody>
</table>


<script src="/socket.io/socket.io.js"></script>  <!--  LLamamos a io para ejecutarlo aca en el frontend -->
<script src="/js/websockets.js"></script>   <!-- Aca le puedo pasar nuestro js publico que es donde tenemos configurado nuestro server io -->
